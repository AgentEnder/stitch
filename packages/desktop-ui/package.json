{
  "name": "@bscotch/stitch-ui",
  "version": "0.3.3",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/bscotch/stitch.git",
    "directory": "packages/desktop-ui"
  },
  "exports": {
    ".": {
      "import": "./build/handler.js"
    }
  },
  "scripts": {
    "build": "svelte-kit sync && vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "dev": "svelte-kit sync && cd ../.. && concurrently -n servers,deps \"pnpm turbo run serve --only --parallel --filter=stitch-ui --filter=stitch-server\" \"pnpm turbo run watch --only --parallel --filter=^stitch-server...\"",
    "preview": "vite preview",
    "serve": "svelte-kit sync && vite dev --force",
    "watch": "svelte-kit sync && vite build --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@bscotch/gamemaker-releases": "^0.3.1",
    "@bscotch/stitch-server": "workspace:*",
    "@sveltejs/adapter-node": "1.1.7",
    "@sveltejs/kit": "1.5.0",
    "@trpc/client": "10.10.0",
    "@trpc/server": "10.10.0",
    "@types/uuid": "^9.0.0",
    "@xstate/svelte": "2.0.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte": "2.17.0",
    "fuse.js": "^6.6.2",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "rimraf": "^4.1.2",
    "superjson": "1.12.2",
    "svelte": "3.55.1",
    "svelte-check": "3.0.3",
    "svelte-preprocess": "5.0.1",
    "turbo": "^1.7.4",
    "typescript": "4.9.5",
    "uuid": "^9.0.0",
    "vite": "4.1.1",
    "xstate": "4.35.4"
  },
  "trebuchet": {
    "keep": {
      "dependencies": [
        "svelte"
      ]
    }
  }
}
